		}
	)
end

function HealthApp:didMount()
	local hum = self.props.humanoid

	hum:GetPropertyChangedSignal("Health"):Connect(function()
		self.healthMotor:setGoal(Flipper.Spring.new(hum.Health,
		{
			frequency = 1,
			dampingRatio = 1,
		}))
	end)
end

function HealthApp:willUnmount()
	
end

